# find manuscripts by language
SELECT ?ms ?msLabel ?language ?languageLabel
WHERE {
# bind query variables
  BIND(p:P10 AS ?languageAsRecorded).
  BIND(pq:P24 AS ?hasLanguage).
# statement: manuscript has language recorded as literal string value
  ?ms ?languageAsRecorded ?languageString .
# statement: string value has qualifier language authority value
  ?languageString ?hasLanguage ?language .
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE]". }
  }
ORDER by ?languageLabel
