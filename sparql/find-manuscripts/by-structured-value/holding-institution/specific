# find manuscript records held by "X"
SELECT ?record ?recordLabel ?institution ?institutionLabel
WHERE {
# bind query variables
  BIND(p:P47 AS ?holdingInstitutionAsRecorded).
  BIND(pq:P35 AS ?hasHoldingInstitution).
  BIND(wdt:P48 AS ?hasQID).
# statement: manuscript record has holding institution recorded as literal string value
  ?record ?holdingInstitutionAsRecorded ?institutionString .
# statement: string value has qualifier institution authority value
  ?institutionString ?hasHoldingInstitution ?institution .
# statement: authority value represented by QID value for "University of Pennsylvania"
  ?institution ?hasQID "Q49117"^^xsd:string.
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE]". }
  }
