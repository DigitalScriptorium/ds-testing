# find records for non-dated manuscripts
SELECT ?record ?recordLabel
WHERE {
# bind query variable for dated property (P15)
  BIND(wdt:P15 AS ?dated).
# statement: manuscripts that are non-dated/datable (Q12)
  ?record ?dated wd:Q12.
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE]". }
  }
